{
  "name": "normalize.ts",
  "version": "0.0.1",
  "description": "Inline styled normalize.css written in TypeScript.",
  "main": "dist/normalize.js",
  "types": "dist/normalize.d.ts",
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "webpack",
    "watch": "webpack -w",
    "prepublishOnly": "tsc",
    "test": "jest"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/shotastage/normalize.ts.git"
  },
  "author": "Shota Shimazu <hornet.live.mf@gmail.com> (https://folio.digital-nichan.com/)",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/shotastage/normalize.ts/issues"
  },
  "jest": {
    "moduleNameMapper": {
      "^#/(.+)": "<rootDir>/src/$1"
    },
    "moduleFileExtensions": [
      "ts",
      "js"
    ],
    "transform": {
      "^.+\\.ts$": "ts-jest"
    },
    "globals": {
      "ts-jest": {
        "tsConfig": "tsconfig.json"
      }
    },
    "testMatch": [
      "**/tests/**/*.test.ts"
    ]
  },
  "homepage": "https://github.com/shotastage/normalize.ts#readme",
  "private": false,
  "dependencies": {
    "@types/node": "^16.11.7",
    "normalize.css": "^8.0.1",
    "styled-components": "^5.3.3",
    "typescript": "^4.4.4"
  },
  "devDependencies": {
    "@types/jest": "^27.0.2",
    "@types/styled-components": "^5.1.15",
    "jest": "^27.3.1",
    "react": "^17.0.2",
    "ts-jest": "^27.0.7",
    "ts-loader": "^9.2.6",
    "ts-node": "^10.4.0",
    "webpack": "^5.64.1",
    "webpack-cli": "^4.9.1"
  }
}
